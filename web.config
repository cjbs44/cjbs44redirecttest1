<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="SimpleURL" stopProcessing="true">
                    <match url="(.*)" />
                    <action type="Redirect" url="http://www.clanwilson.net/" appendQueryString="false" redirectType="Permanent" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
